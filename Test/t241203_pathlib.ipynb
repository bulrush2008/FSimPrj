    "import pathlib\n",
    "\n",
    "usr = pathlib.PurePosixPath(\"/usr\")\n",
    "print(usr, type(usr))\n",
    "\n",
    "usr_local = usr/\"local\"; print(usr_local)\n",
    "\n",
    "usr_share = usr / pathlib.PurePosixPath(\"share\"); print(usr_share)\n",
    "\n",
    "root = usr / \"..\"; print(root)"
